{
  "name": "node-p2pool-scanner",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "start": "DEBUG='node-p2pool-scanner:* node-p2pool-scanner:*:*' NODE_ENV=development nodemon --exec 'node --experimental-modules --experimental-json-modules --use-strict' ./app.mjs",
